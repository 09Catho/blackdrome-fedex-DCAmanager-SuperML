{
  "version": "1.0",
  "model_type": "logistic_regression",
  "description": "Recovery prediction and prioritization model for DCA cases",
  "bias": -0.4,
  "weights": {
    "ageing": -1.2,
    "log_amount": 0.6,
    "attempts": 0.5,
    "staleness": -0.8,
    "dispute": -1.0,
    "ptp_active": 0.7
  },
  "reason_mappings": {
    "ageing": "High ageing reduces recovery probability",
    "log_amount": "Higher amount increases priority",
    "attempts": "More contact attempts improve recovery",
    "staleness": "Days since last update reduces priority",
    "dispute": "Active dispute reduces recovery probability",
    "ptp_active": "Active payment promise increases recovery"
  }
}
